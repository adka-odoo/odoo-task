<?xml version="1.0"?>
<odoo>
    <record id="sales_targets_list_view" model="ir.ui.view">
        <field name="name">sales.targets.view.tree</field>
        <field name="model">sales.targets</field>
        <field name="arch" type="xml">
            <tree string="Channel" sample="1" editable="bottom">
                <field name="name"/>
                <field name="quarter_start_date" />
                <field name="quarter_end_date" />
                <field name="target_amount" />
            </tree>
        </field>
    </record>
</odoo>